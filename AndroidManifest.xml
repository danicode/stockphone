<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.stockphone"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="7" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:icon="@drawable/ic_launcher_stockphone"
        android:label="@string/app_name" >
        <activity
            android:name="com.view.initial.FrmInitial"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.view.login.FrmLogin" >
        </activity>
        <activity android:name="com.view.main.FrmMain" >
        </activity>
        <activity android:name="com.view.about.FrmAboutStockPhone" >
        </activity>
        <activity
            android:name="com.view.stock.FrmStock"
            android:configChanges="keyboard|keyboardHidden|orientation" >
        </activity>
        <activity android:name="com.view.requisition.FrmRequisition" >
        </activity>
        <activity android:name="com.view.summary.FrmSummary" >
        </activity>
    </application>

</manifest>